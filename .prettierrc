{
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrder": [
    "react",
    "<THIRD_PARTY_MODULES>",
    "^./(.*)$",
    "^../(.*)$",
    "@pages/",
    "@router/",
    "@routes/",
    "@components/",
    "@store/",
    "@api/",
    "@hooks/",
    "@utils/",
    "@interfaces/",
    "@enums/",
    "types/",
    "@constants/",
    "@assets/",
    "@UI/"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "printWidth": 80,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "arrowParens": "avoid",
  "proseWrap": "always",
  "jsxSingleQuote": false
}
